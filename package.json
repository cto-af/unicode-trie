  },
  "license": "MIT",
  "dependencies": {
    "fflate": "^0.8.2"
  },
  "devDependencies": {
    "@cto.af/eslint-config": "5.1.13",
    "@types/node": "22.13.9",
    "c8": "10.1.3",
    "eslint": "9.21.0",
    "eslint-plugin-mocha": "10.5.0",
    "mocha": "11.1.0",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@10.6.0",
  "engines": {
    "node": ">=18"
  }
