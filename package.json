{
  "name": "@cto.af/unicode-trie",
  "version": "2.0.1",
  "description": "Quick fork of unicode-trie for modernity",
  "type": "module",
  "exports": {
    ".": {
      "default": "./index.js",
      "types": "./types/index.d.ts"
    },
    "./builder": {
      "default": "./builder.js",
      "types": "./types/builder.d.ts"
    }
  },
  "scripts": {
    "test": "c8 mocha",
    "lint": "eslint .",
